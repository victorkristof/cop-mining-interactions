    "def write_text(text, name_file):\n",
    "    \"\"\" Write 'text' (list of strings) in the 'name_file'  \"\"\"\n",
    "    outF = open(name_file, \"w\")\n",
    "    for line in text:\n",
    "    # write line to output file\n",
    "        outF.write(line)\n",
    "        outF.write(\"\\n\")\n",
    "    outF.close()"
